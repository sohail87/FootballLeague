@model IEnumerable<FootballLeagueTable.Domain.MatchResult>

<div>
    <h1>@ViewBag.Title</h1>

    <p>Premier League 2015-2016 </p>

    <table class="table table-striped table-hover">
        <tr>
            <th>Date</th>
            <th>Home</th>
            <th>FT Score</th>
            <th>Away</th>
            <th>HT Score</th>
            <th>Referee</th>

        </tr>
        @if (Model.Any())
        {
            foreach (var result in Model)
            {
                @Html.Partial("_PartialMatchResult", result)
                ;
            }
        }
    </table>
</div>